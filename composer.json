{
    "name": "nawawi/monodb",
    "description": "A simple flat-file key-value data structure store, used as a database, cache and message broker.",
    "keywords": [
        "database",
        "database-storage",
        "key-value database",
        "flat-file",
        "flat-file-db",
        "flat-file database",
        "flatfile database",
        "flat-file-storage",
        "monodb",
        "serverless",
        "document",
        "php"
    ],
    "type": "library",
    "license": "GPL-3.0",
    "homepage": "https://monodb.io",
    "authors": [
        {
            "name": "Nawawi Jamili",
            "email": "nawawi@rutweb.com",
            "homepage": "https://rutweb.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-json": "*",
        "symfony/var-exporter": "^5.0"
    },
    "suggest": {
        "ext-zend-opcache": "Recommended for better performance"
    },
    "autoload": {
        "psr-4": {
            "MonoDB\\": "src/"
        },
        "classmap": ["src/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0.x-dev"
        }
    }
}
